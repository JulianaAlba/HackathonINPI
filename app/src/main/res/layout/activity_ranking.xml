<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Ranking">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" />
    </ScrollView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#FFFFFF"
            app:srcCompat="@drawable/rank" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="20sp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView45"
                android:layout_width="21dp"
                android:layout_height="50sp"
                android:layout_weight="1"
                android:text="     4ยบ"
                android:textAlignment="viewStart"
                android:textColor="@color/primarydarken01"
                android:textSize="20sp" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="95dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="#FFFFFF"
                app:srcCompat="@drawable/rank1" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="50sp"
                android:layout_weight="1"
                android:text="Valter Silva"
                android:textColor="@color/primarydarken01"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/textView26"
                android:layout_width="10sp"
                android:layout_height="20sp"
                android:layout_weight="1"
                android:background="@color/warning"
                android:text="     4.1"
                android:textAlignment="viewStart"
                android:textColor="@color/primarydarken01" />

            <Space
                android:layout_width="30sp"
                android:layout_height="wrap_content"
                android:layout_weight="1" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView22"
                android:layout_width="21dp"
                android:layout_height="50sp"
                android:layout_weight="1"
                android:text="     5ยบ"
                android:textAlignment="viewStart"
                android:textColor="@color/primarydarken01"
                android:textSize="20sp" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="93dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="#FFFFFF"
                app:srcCompat="@drawable/rank2" />

            <TextView
                android:id="@+id/textView23"
                android:layout_width="wrap_content"
                android:layout_height="50sp"
                android:layout_weight="1"
                android:text="Ana Carolina"
                android:textColor="@color/primarydarken01" />

            <TextView
                android:id="@+id/textView31"
                android:layout_width="10sp"
                android:layout_height="20sp"
                android:layout_weight="1"
                android:background="@color/warning"
                android:text="     3.9"
                android:textColor="@color/primarydarken01" />

            <Space
                android:layout_width="30sp"
                android:layout_height="wrap_content"
                android:layout_weight="1" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView24"
                android:layout_width="21dp"
                android:layout_height="50sp"
                android:layout_weight="1"
                android:text="     6ยบ"
                android:textColor="@color/primarydarken01"
                android:textSize="20sp" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="95dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="#FFFFFF"
                app:srcCompat="@drawable/rank3" />

            <TextView
                android:id="@+id/textView25"
                android:layout_width="wrap_content"
                android:layout_height="50sp"
                android:layout_weight="1"
                android:text="Amanda Vieira"
                android:textColor="@color/primarydarken01" />

            <TextView
                android:id="@+id/textView46"
                android:layout_width="10sp"
                android:layout_height="20sp"
                android:layout_weight="1"
                android:background="@color/warning"
                android:text="     3.8"
                android:textColor="@color/primarydarken01" />

            <Space
                android:layout_width="30sp"
                android:layout_height="wrap_content"
                android:layout_weight="1" />
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>