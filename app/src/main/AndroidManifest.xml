<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.impulse">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Impulse">
        <activity android:name=".QuestoesSimulado"></activity>
        <activity android:name=".Ranking" />
        <activity android:name=".CadastroAreaDeCompatibilidade" />
        <activity android:name=".Simulado" />
        <activity android:name=".Audio" />
        <activity android:name=".Video" />
        <activity android:name=".Feed" />
        <activity android:name=".Perfil" />
        <activity android:name=".Acessar" />
        <activity android:name=".Cadastrar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>